{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "apps",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/tsconfig.json"
  },
  "projects": {
    "server-bznav": {
      "type": "application",
      "root": "apps/server-bznav",
      "entryFile": "main",
      "sourceRoot": "apps/server-bznav/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server-bznav/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}